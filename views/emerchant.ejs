<html>
<head>
    <% include head/default.ejs %>
</head>
<body>
<% include nav/default.ejs %>
<div align="center"><h2>Fillin Your Order Information Below</h2></div>
<form id="submit_order" class="form-horizontal"  action="/order/new">

    <div id="sender" class="table-bordered center-block " style="padding: 10px; width: 500px;">
        <div class="form-group">
            <label for="senderName" class="col-sm-3 control-label">senderName</label>
            <div class="col-sm-9">
                <input type="text" name="senderName" class="form-control" id="senderName">
            </div>
        </div>
        <div class="form-group">
            <label for="senderPhone" class="col-sm-3 control-label">senderPhone</label>
            <div class="col-sm-9">
                <input type="number" name="senderPhone" class="form-control" id="senderPhone">
            </div>
        </div>
        <div class="form-group">
            <label for="senderProvince" class="col-sm-3 control-label">senderProvince</label>
            <div class="col-sm-9">
                <input type="text" name="senderProvince" class="form-control" id="senderProvince">
            </div>
        </div>
        <div class="form-group">
            <label for="senderCity" class="col-sm-3 control-label">senderCity</label>
            <div class="col-sm-9">
                <input type="text" name="senderCity" class="form-control" id="senderCity">
            </div>
        </div>
        <div class="form-group">
            <label for="senderDistrict" class="col-sm-3 control-label">senderDistrict</label>
            <div class="col-sm-9">
                <input type="text" name="senderDistrict" class="form-control" id="senderDistrict">
            </div>
        </div>
        <div class="form-group">
            <label for="senderAddress" class="col-sm-3 control-label">senderAddress</label>
            <div class="col-sm-9">
                <input type="text" name="senderAddress" class="form-control" id="senderAddress">
            </div>
        </div>
    </div><br/>

    <div id="receiver" class="table-bordered center-block" style="padding: 10px; width: 500px;">
        <div class="form-group">
            <label for="receiverName" class="col-sm-3 control-label">receiverName</label>
            <div class="col-sm-9">
                <input type="text" name="receiverName" class="form-control" id="receiverName">
            </div>
        </div>
        <div class="form-group">
            <label for="receiverPhone" class="col-sm-3 control-label">receiverPhone</label>
            <div class="col-sm-9">
                <input type="number" name="receiverPhone" class="form-control" id="receiverPhone">
            </div>
        </div>
        <div class="form-group">
            <label for="receiverProvince" class="col-sm-3 control-label">receiverProvince</label>
            <div class="col-sm-9">
                <input type="text" name="receiverProvince" class="form-control" id="receiverProvince">
            </div>
        </div>
        <div class="form-group">
            <label for="receiverCity" class="col-sm-3 control-label">receiverCity</label>
            <div class="col-sm-9">
                <input type="text" name="receiverCity" class="form-control" id="receiverCity">
            </div>
        </div>
        <div class="form-group">
            <label for="receiverDistrict" class="col-sm-3 control-label">receiverDistrict</label>
            <div class="col-sm-9">
                <input type="text" name="receiverDistrict" class="form-control" id="receiverDistrict">
            </div>
        </div>
        <div class="form-group">
            <label for="receiverAddress" class="col-sm-3 control-label">receiverAddress</label>
            <div class="col-sm-9">
                <input type="text" name="receiverAddress" class="form-control" id="receiverAddress">
            </div>
        </div>
    </div><br/>

    <div id="orderInfo" class="table-bordered center-block" style="padding: 10px; width: 500px;">
        <div class="form-group">
            <label for="orderNumber" class="col-sm-3 control-label">orderNumber</label>
            <div class="col-sm-9">
                <input type="number" name="orderNumber" class="form-control" id="orderNumber">
            </div>
        </div>
        <div class="form-group">
            <label for="content" class="col-sm-3 control-label">content</label>
            <div class="col-sm-9">
                <input type="text" name="content" class="form-control" id="content">
            </div>
        </div>
        <div class="form-group">
            <label for="weight" class="col-sm-3 control-label">weight</label>
            <div class="col-sm-9">
                <input type="number" name="weight" class="form-control" id="weight">
            </div>
        </div>
    </div><br/>

    <button class="btn btn-success center-block">
        Submit Your Order
    </button>
    <!--<nav id="submitOrderButton" class="navbar navbar-default navbar-fixed-bottom">
        <div class="navbar-right navbar-form container-fluid">
            <button class="btn btn-success">
                Submit Your Order
            </button>
        </div>
    </nav>-->
</form>
</body>
</html>